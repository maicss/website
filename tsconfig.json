{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types/", "./types", "./node_modules"],
    "types": ["vite/client"],
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
    "verbatimModuleSyntax": true,
    "noEmit": true
  },
  "include": ["./.vitepress/**/*"]
}
